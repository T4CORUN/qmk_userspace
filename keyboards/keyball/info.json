{
    "manufacturer": "Yowkees",
    "maintainer": "t4corun",
    "url": "https://github.com/Yowkees/keyball",
    "usb": {
        "vid": "0x5957"
    },

    "processor": "atmega32u4",
    "bootloader": "caterina",

    "matrix_pins": {
        "rows": [ "F4", "F5", "F6", "F7" ],
        "cols": [ "D4", "C6", "D7", "E6", "B4", "B5" ]
    },
    "diode_direction": "COL2ROW",
    "debounce": 5,

    "build": {
        "lto": true
    },

    "qmk": {
        "locking": {
            "enabled": false,
            "resync": false
        },
        "tap_keycode_delay": 5
    },

    "features": {
        "bootmagic": false,
        "extrakey": false,
        "console": false,
        "command": false,
        "nkro": false,
        "backlight": false,
        "audio": false,
        "rgblight": false,
        "rgb_matrix": false,
        "oled": false,
        "space_cadet": false,
        "grave_esc": false,
        "magic": false,
        "pointing_device": true
    },

    "split": {
        "enabled": true,
        "usb_detect": true,
        "serial": {
            "pin": "D2"
        },
        "transport": {
            "sync": {
                "matrix_state": true,
                "oled": true
            }
        }
    },

    "rgblight": {
        "max_brightness": 150,
        "brightness_steps": 15,
        "hue_steps": 17,
        "saturation_steps": 17,
        "sleep": true,
        "animations": {
            "breathing": true,
            "rainbow_mood": true,
            "rainbow_swirl": true,
            "snake": true,
            "knight": true,
            "christmas": true,
            "static_gradient": true,
            "rgb_test": true,
            "alternating": true,
            "twinkle": true
        }
    },

    "rgb_matrix": {
        "driver": "ws2812",
        "sleep": true
    },

    "ws2812": {
        "pin": "D3"
    },

    "mouse_key": {
        "enabled": false
    }

}
